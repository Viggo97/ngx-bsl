{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "paths": {
      "ngx-bsl": [
        "./dist/ngx-bsl"
      ],
      "@ngx-bsl-lib": [
        "projects/ngx-bsl/src/public-api.ts"
      ]
    },
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "module": "preserve",
    "baseUrl": "./",
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "typeCheckHostBindings": true,
    "strictTemplates": true
  },
  "files": [],
  "references": [
    {
      "path": "./projects/ngx-bsl/tsconfig.lib.json"
    },
    {
      "path": "./projects/ngx-bsl/tsconfig.spec.json"
    },
    {
      "path": "./projects/ngx-bsl-test-app/tsconfig.app.json"
    },
    {
      "path": "./projects/ngx-bsl-test-app/tsconfig.spec.json"
    }
  ]
}
